<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.LoginPanelController">
   <children>
      <Label layoutX="106.0" layoutY="14.0" text="Login" textFill="#e11d1d">
         <font>
            <Font name="System Bold Italic" size="27.0" />
         </font>
      </Label>
      <Label layoutX="21.0" layoutY="86.0" text="UserName" />
      <Label layoutX="21.0" layoutY="142.0" text="Password" />
      <PasswordField fx:id="txtPassword" layoutX="82.0" layoutY="138.0" promptText="Password" />
      <TextField fx:id="txtUserNAME" layoutX="82.0" layoutY="82.0" promptText="UserName" />
      <Button layoutX="177.0" layoutY="228.0" mnemonicParsing="false" text="Login" />
      <Button layoutX="71.0" layoutY="228.0" mnemonicParsing="false" onAction="#ExitBtn" text="Exit" />
   </children>
</AnchorPane>
